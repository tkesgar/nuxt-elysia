{
  "name": "nuxt-elysia",
  "version": "0.2.0",
  "description": "Mount Elysia app in Nuxt",
  "repository": "tkesgar/nuxt-elysia",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "dev": "nuxi dev playground",
    "dev:bun": "bun --bun nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:build:bun": "NITRO_PRESET=bun nuxi build playground",
    "dev:start": "node playground/.output/server/index.mjs",
    "dev:start:bun": "bun playground/.output/server/index.mjs",
    "lint": "eslint .",
    "test": "vitest",
    "test:bun": "bun test bun",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^4.0.3"
  },
  "peerDependencies": {
    "elysia": ">= 1.2",
    "@elysiajs/eden": ">= 1.2"
  },
  "devDependencies": {
    "@elysiajs/eden": "^1.3.3",
    "@elysiajs/node": "^1.3.1",
    "@elysiajs/swagger": "^1.3.1",
    "@nuxt/devtools": "^2.6.3",
    "@nuxt/eslint-config": "^1.9.0",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.0.3",
    "@nuxt/test-utils": "^3.19.2",
    "@types/bun": "^1.2.21",
    "@types/ejs": "^3.1.5",
    "cheerio": "^1.1.2",
    "ejs": "^3.1.10",
    "elysia": "^1.3.20",
    "eslint": "^9.34.0",
    "nuxt": "^4.0.3",
    "typescript": "~5.9.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.6"
  },
  "unbuild": {
    "rollup": {
      "inlineDependencies": [
        "ejs"
      ]
    }
  }
}
